# Multiplication Table
num = int(input("Enter a number: "))
count = 1
while count <=20:
    product = num*count
    print(num,"x",count,"=",product)
    count = count + 1
    
# Inverted Multiplication table
# Multiplication Table
num = int(input("Enter a number: "))
count = 20
while count >=1:
    product = num*count
    print(num,"x",count,"=",product)
    count = count - 1
